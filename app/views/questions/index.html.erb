<table class="index_table">
    <tr>
        <th><h2>Top Questions</h2></th>
        <th><h3><%= link_to "Ask Question?", new_question_path %></h3></th>
    </tr>
</table>
<% @questions.each do |question| %>
<div class="session" data-question-id="<%= question.id %>">
    <p class="divisor"></p>
    <div class="q_votes">
        <%= question.vote %> <br>
        <div>votes</div>
    </div>
    <div class="status">
        <%= question.answers.size %> <br>
        <div>answers</div>
    </div>
    <div class="views">
        <%= rand(1..1000) %> <br>
        <div>views</div>
    </div>

    <div class="voting">
        <p><%= link_to "Up", upvote_question_path(question) %></p>
        <p><%= link_to "Down" %></p>
    </div>

    <div class="questions">
        <p><%= link_to question.title, question_path(question) %></p>
    </div>
</div>
<% end %>
<table class="index_table">
    <tr>
        <th><h2>Top Questions</h2></th>
        <th><h3><%= link_to "Ask Question?", new_question_path %></h3></th>
    </tr>
</table>
<% @questions.each do |question| %>
<div class="cp" data-question-id="<%= question.id %>">
    <p class="divisor"></p>
    <div class="q_votes">
        <%= rand(1..100) %> <br>
        <div>votes</div>
    </div>
    <div class="status">
        <%= rand(1..10) %> <br>
        <div>answers</div>
    </div>
    <div class="views">
        <%= rand(1..1000) %> <br>
        <div>views</div>
    </div>
    <div class="questions">
        <p><%= link_to question.title, question_path(question) %></p>
        <div class="voting">
            <p><%= link_to "Upvote" %></p>
            <p><%= link_to "Downvote" %></p>
        </div>
    </div>
</div>
<% end %>